<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Badgeuse Self Signal{% endblock %}</title>
    {% block stylesheets %}
        {# Google Font #}
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Cairo:wght@400;700&display=swap" rel="stylesheet">
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
<header>
    <div class="container">
        <h1>
            <a href="{{ path('home_index') }}" title="Badgeuse Self Signal">{% block titre %}Badgeuse Self Signal{% endblock %}</a>
        </h1>
        {% include('_nav.html.twig') %}
    </div>
</header>
<main>
    <div class="container">
        {% block body %}{% endblock %}
    </div>
</main>
</body>
</html>
